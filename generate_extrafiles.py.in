    @SWIG_WITH_PYTHON2_WRAPPERS_TRUE@"%{prefix}/lib/emdros/_EmdrosPy.so.*", #MAIN
    @SWIG_WITH_PYTHON3_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPy3.py",  #MAIN
    @SWIG_WITH_PYTHON3_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPy3.pyc",  #MAIN
    @SWIG_WITH_PYTHON3_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPy3.pyo",  #MAIN
    @SWIG_WITH_PYTHON3_WRAPPERS_TRUE@"%{prefix}/lib/emdros/_EmdrosPy3.so.*",  #MAIN
    @SWIG_WITH_PHP5_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP.php", # MAIN
    @SWIG_WITH_PHP5_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP.ini", # MAIN
    @SWIG_WITH_PHP5_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP.so.*", # MAIN
    @SWIG_WITH_PHP7_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP7.php", # MAIN
    @SWIG_WITH_PHP7_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP7.ini", # MAIN
    @SWIG_WITH_PHP7_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP7.so.*", # MAIN
    @SWIG_WITH_JAVA_WRAPPERS_TRUE@"%{prefix}/lib/emdros/jemdros.jar", # MAIN
    @SWIG_WITH_JAVA_WRAPPERS_TRUE@"%{prefix}/lib/emdros/libjemdros.so.*", # MAIN
    @SWIG_WITH_CSHARP_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosDotNet.dll", #MAIN
    @SWIG_WITH_CSHARP_WRAPPERS_TRUE@"%{prefix}/lib/emdros/libcsemdros.so.*", # MAIN
]

extrafiles_emdros_devel_list = [
    @SWIG_WITH_PYTHON2_WRAPPERS_TRUE@"%{prefix}/lib/emdros/_EmdrosPy.la",
    @SWIG_WITH_PYTHON2_WRAPPERS_TRUE@"%{prefix}/lib/emdros/_EmdrosPy.so",
    @SWIG_WITH_PYTHON3_WRAPPERS_TRUE@"%{prefix}/lib/emdros/_EmdrosPy3.la",
    @SWIG_WITH_PYTHON3_WRAPPERS_TRUE@"%{prefix}/lib/emdros/_EmdrosPy3.so",
    @SWIG_WITH_PHP7_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP7.la",
    @SWIG_WITH_PHP7_WRAPPERS_TRUE@"%{prefix}/lib/emdros/EmdrosPHP7.so",
    @SWIG_WITH_JAVA_WRAPPERS_TRUE@"%{prefix}/lib/emdros/libjemdros.la",
    @SWIG_WITH_JAVA_WRAPPERS_TRUE@"%{prefix}/lib/emdros/libjemdros.so",
    @SWIG_WITH_CSHARP_WRAPPERS_TRUE@"%{prefix}/lib/emdros/libcsemdros.la",
    @SWIG_WITH_CSHARP_WRAPPERS_TRUE@"%{prefix}/lib/emdros/libcsemdros.so",
    @WX_DO_UNICODE_TRUE@"%{prefix}/lib/libemdrosguiu.a",
]


def write_file(outfilename, filename_list):
    fout = open(outfilename, "wb")

    for filename in filename_list:
        fout.write(filename + "\n")

    fout.close()


def doIt():
    write_file("extrafiles-emdros.txt", extrafiles_emdros_list)
    write_file("extrafiles-emdros-devel.txt", extrafiles_emdros_devel_list)


if __name__ == '__main__':
    doIt()
