GO


//
// The "db" object type is given to every database that must be
// recognized as belonging to the eBibel.
// 
// There must only be one db object in any given database,
// and it must have a "dbname" string which uniquely identifies this
// database by that name in the entire set of resources.
//
// The "dbname" ends up in the DBEnv::m_env_map in the desktop app.
//
CREATE OBJECT TYPE
WITH SINGLE RANGE OBJECTS
[db
     dbname : STRING;
     friendly_dbname : STRING;
     bible_parts : LIST OF bible_part_e;
     priority : INTEGER; // When displaying more than one at a time.
     fts_search : boolean_t;
     language : language_e;
     dbtype : dbtype_e;
]
GO
